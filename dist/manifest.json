{
  "name": "New window history",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "activeTab", "<all_urls>"]
}